{
    "name": "react-tyspecript-rtl-styled-components-starter-kit",
    "version": "1.0.0",
    "description": "React Typescript React Testing Library and Styled component starter kit",
    "main": "index.js",
    "scripts": {
        "lint": "run-p -c lint:*",
        "lint:css": "stylelint 'src/**/*.tsx'",
        "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
        "test": "run-p type-check test:jest",
        "test:update": "run-p type-check 'test:jest -- -u'",
        "test:watch": "run-p 'type-check -- -w' 'test:jest -- --watch'",
        "type-check": "tsc",
        "test:jest": "jest --config config/jest/jest.config.js",
        "build": "cross-env BABEL_ENV=production webpack --progress --config config/webpack/webpack.prod.js",
        "build:analyze": "cross-env BUNDLE_ANALYZE=true npm run build -- --watch",
        "watch": "webpack-dev-server --open --config config/webpack/webpack.dev.js"
    },
    "author": "Kaustav Chakraborty",
    "license": "ISC",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "axios": "^0.21.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.1",
        "@babel/preset-typescript": "^7.9.0",
        "@commitlint/cli": "^12.0.1",
        "@commitlint/config-conventional": "^12.0.1",
        "@hot-loader/react-dom": "^16.13.0",
        "@svgr/webpack": "^5.3.0",
        "@testing-library/jest-dom": "^5.1.1",
        "@testing-library/react": "^10.0.1",
        "@types/jest": "^25.1.4",
        "@types/react": "^16.9.25",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@types/styled-components": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "axios-mock-adapter": "^1.18.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.10.7",
        "circular-dependency-plugin": "^5.2.0",
        "clean-webpack-plugin": "^3.0.0",
        "commitizen": "^4.0.3",
        "compression-webpack-plugin": "^3.1.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^7.0.2",
        "cz-conventional-changelog": "^3.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.1",
        "fork-ts-checker-webpack-plugin": "^4.1.1",
        "html-webpack-plugin": "^4.0.4",
        "husky": "^4.2.3",
        "jest": "^25.1.0",
        "jest-styled-components": "^7.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.2",
        "react-hot-loader": "^4.12.20",
        "stylelint": "^13.2.1",
        "stylelint-config-prettier": "^8.0.1",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-custom-processor-loader": "^0.6.0",
        "stylelint-prettier": "^1.1.2",
        "thread-loader": "^2.1.3",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1",
        "webpack-bundle-analyzer": "^3.6.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2"
    }
}
